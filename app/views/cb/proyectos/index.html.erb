<h1>Listing cb_proyectos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cb_proyectos.each do |cb_proyecto| %>
      <tr>
        <td><%= cb_proyecto.name %></td>
        <td><%= link_to 'Show', cb_proyecto %></td>
        <td><%= link_to 'Edit', edit_cb_proyecto_path(cb_proyecto) %></td>
        <td><%= link_to 'Destroy', cb_proyecto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proyecto', new_cb_proyecto_path %>
